!!! 5
html
  head
    title
      if title && project.name
        #{title} | #{project.name}
      else if project.name
        #{project.name}
      else if title 
        #{title}
    meta(charset='utf-8')
    link(rel="stylesheet", href="docs.css")
    if previewCSS
      style(type='text/preview') !{previewCSS}
    if previewJS
      script(type='text/preview') !{previewJS}
  body
    nav.navbar
      .container
        a.brand(href="index.html") #{project.name}
        ul.nav
          each links, folder in project.menu
            if folder == './'
              each link in links
                li
                  a(href="#{link.href}")
                    #{link.name}
            else
              li
                a.dropdown-toggle(href="#") #{folder}
                ul.dropdown
                  each link in links
                    li
                      a(href="#{link.href}")
                        #{link.name}
    section.container
      each section, i in sections
        article.section(id="section-#{i + 1}")
          .docs
            h1 #{section.title}
            !{section.docs}
          if section.code
            .code
              pre
                code !{section.code}
    script(src="docs.js")
